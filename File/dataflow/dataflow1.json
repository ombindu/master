{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dsranks",
						"type": "DatasetReference"
					},
					"name": "sourceemp"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sinkse",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "rankemp"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empid as string,",
				"          name as string,",
				"          gender as string,",
				"          salary as string,",
				"          dep as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceemp",
				"sourceemp rank(desc(salary, true),",
				"     output(ranking as long),",
				"     dense: true) ~> rankemp",
				"rankemp sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          empid as string,",
				"          name as string,",
				"          country as string,",
				"          department as string",
				"     ),",
				"     partitionFileNames:['emprank'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          empid,",
				"          name",
				"     ),",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}